<body>
	
	<!-- Navbar STart -->
	<header id="topnav" class="defaultscroll sticky">
		<div class="container">
			<!-- Logo container-->
			<div>
				<a class="logo" href="index.html">
					<img src="/static/images/logo-dark.png" class="l-dark" height="24" alt="">
					<img src="/static/images/logo-light.png" class="l-light" height="24" alt="">
				</a>
			</div>                 
			<div class="buy-button">
				<a href="/form" target="">
					<div class="btn btn-primary login-btn-primary">Ingresar</div>
					<div class="btn btn-light login-btn-light">Ingresar</div>
				</a>
			</div><!--end login button-->
			<!-- End Logo container-->
	
				<div class="buy-menu-btn d-none">
					<a href="/form" target="" class="btn btn-primary">Ingresar</a>
				</div><!--end login button-->

		</div><!--end container-->
	</header><!--end header-->
	<!-- Navbar End -->
	
	<!-- Hero Start -->
	<section class="bg-half-260 w-100 d-table" data-jarallax='{"speed": 0.5}' style="background: url('/static/images/apuba_fachada.png') center center;">
		<div class="bg-overlay bg-overlay-gradient"></div>
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-lg-10 text-center">
					<div class="title-heading">
						<h4 class="text-success mb-3">07 de Abril, 2022</h4>
						<h1 class="display-4 title-dark text-white font-weight-bold mb-3">Elecciones Apuba 2022</h1>
						<p class="para-desc title-dark mx-auto text-light">Votación para el cargo de Secretario General de la Asociación del Personal de la UBA.</p>
						
						<div class="row">
							<div class="col-md-12 text-center">
								<div id="eventdown"></div>
							</div>
						</div>
						<div class="mt-4 pt-2">
							<a href="#resultado" class="btn btn-success mt-2 mr-2"><i class="mdi mdi-ticket"></i> Ver los Resultados </a>
						</div>
					</div>
				</div><!--end col-->
			</div><!--end row-->
		</div><!--end container-->
	</section><!--end section-->
	<div id="resultado" class="position-relative">
		<div class="shape overflow-hidden text-footer">
			<div class="text-center bg-white p-4">
				<h5 class="text-dark mb-0">Escrutinio : 07 Abril, 18hs</h5>
			</div>
		</div>
	</div>
	<!-- Hero End -->

	<!-- About START -->
	<section  class="section border-top">
		<div class="container">
		<h2 class="text-dark mb-0" style="text-align: center;">Resultado General</h2>
			<div class="card rounded shadow border-0 bg-light overflow-hidden">
				<div class="row no-gutters align-items-center">
					<div class="col-lg-6">
							{{>parciales}}
	
					</div><!--end col-->
					<div class="col-lg-6">
						<div class="card-body section-title p-md-5">
							<div class="d-flex justify-content-between">
								<div class="media align-items-center">
									<a class="pr-3" href="#">
										<img style="background-color: #31b442;" class="img-fluid avatar avatar-md-sm rounded-circle">
									</a>
									<div class="commentor-detail">
										<h6 class="mb-0"><a href="javascript:void(0)" class="text-dark media-heading">{{verde}} %</a></h6>
										<small class="text-muted">Lista Verde</small>
									</div>
								</div>
							</div>
							<div class="d-flex justify-content-between">
								<div class="media align-items-center">
									<a class="pr-3" href="#">
										<img style="background-color: #bb2c1f;" class="img-fluid avatar avatar-md-sm rounded-circle shadow">
									</a>
									<div class="commentor-detail">
										<h6 class="mb-0"><a href="javascript:void(0)" class="text-dark media-heading">{{roja}} %</a></h6>
										<small class="text-muted">Lista Roja</small>
									</div>
								</div>
							</div>
							<div class="d-flex justify-content-between">
								<div class="media align-items-center">
									<a class="pr-3" href="#">
										<img style="background-color: #d4d4d4;" class="img-fluid avatar avatar-md-sm rounded-circle shadow">
									</a>
									<div class="commentor-detail">
										<h6 class="mb-0"><a href="javascript:void(0)" class="text-dark media-heading">{{blancos}} %</a></h6>
										<small class="text-muted">Votos en Blanco</small>
									</div>
								</div>
							</div>
							<div class="d-flex justify-content-between">
								<div class="media align-items-center">
									<a class="pr-3" href="#">
										<img style="background-color: #777777;" class="img-fluid avatar avatar-md-sm rounded-circle shadow">
									</a>
									<div class="commentor-detail">
										<h6 class="mb-0"><a href="javascript:void(0)" class="text-dark media-heading">{{nulos}}</a></h6>
										<small class="text-muted">Votos Nulos</small>
									</div>
								</div>
							</div>
							</br>
							<div class="d-flex justify-content-between">
								<div class="media align-items-center">
									<div class="commentor-detail">
										<h6 class="mb-0"><a href="javascript:void(0)" class="text-dark media-heading">Porcentaje de mesas escrutadas: {{mesas}}%</a></h6>
									</div>
								</div>
							</div>
						</div>
					</div><!--end col-->
				</div><!--end row-->
			</div><!--end card-->
		</div><!--end container-->
	</section><!--end section-->
	<!-- About End -->

	<!-- Shape Start -->
	<div class="position-relative">
		<div class="shape overflow-hidden text-light">
			<svg viewBox="0 0 2880 48" fill="none" xmlns="http://www.w3.org/2000/svg">
				<path d="M0 48H1437.5H2880V0H2160C1442.5 52 720 0 720 0H0V48Z" fill="currentColor"></path>
			</svg>
		</div>
	</div>
	<!--Shape End-->

	<!-- CTA Start -->
	<section class="section bg-cta" data-jarallax='{"speed": 0.5}' style="background: url('/static/images/apuba_fachada_v02.png') center center;" id="cta">
		<div class="bg-overlay bg-overlay-gradient"></div>
		<div class="container">
			<div class="row justify-content-center">

			</div><!--end row-->
		</div><!--end container-->
	</section><!--end section-->
	<!-- CTA End -->

	<!-- News Start -->
	<section class="section bg-light">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-12 text-center">
					<div class="section-title mb-4 pb-2">
						<h4 class="title mb-4">Resultados por Instituto</h4>
						<p class="text-muted para-desc mx-auto mb-0">Vea los resultados de las elecciones de <span class="text-primary font-weight-bold">Apuba</span> </br>desglosadas por instituto y por urnas.</p>
					</div>
				</div><!--end col-->
			</div><!--end row-->

			<div class="row">
				{{>lista}}
			</div><!--end row-->
		</div><!--end container-->
	</section><!--end section-->
	<!-- End News -->

	<!-- Shape Start -->
	<div class="position-relative">
		<div class="shape overflow-hidden text-footer">
			<svg viewBox="0 0 2880 48" fill="none" xmlns="http://www.w3.org/2000/svg">
				<path d="M0 48H1437.5H2880V0H2160C1442.5 52 720 0 720 0H0V48Z" fill="currentColor"></path>
			</svg>
		</div>
	</div>
	<!--Shape End-->

	<!-- Footer Start -->
	<footer class="footer">
		<div class="container">
			<div class="row">
				<div class="col-lg-2 col-md-4 col-12 mt-4 mt-sm-0 pt-2 pt-sm-0">
						<ul class="list-unstyled social-icon social mb-0 mt-4">
							<li class="list-inline-item"><a href="javascript:void(0)" class="rounded"><i data-feather="facebook" class="fea icon-sm fea-social"></i>  Facebook</a></li>
							<li class="list-inline-item"><a href="javascript:void(0)" class="rounded"><i data-feather="instagram" class="fea icon-sm fea-social"></i>  Instagram</a></li>
							<li class="list-inline-item"><a href="javascript:void(0)" class="rounded"><i data-feather="twitter" class="fea icon-sm fea-social"></i>  Twitter</a></li>
						</ul>
				</div>
			</div><!--end row-->
		</div><!--end container-->
	</footer><!--end footer-->
	<footer class="footer footer-bar">
		<div class="container text-center">
			<div class="row align-items-center">
				<div class="col-sm-6">
					<div class="text-sm-left">
						<p class="mb-0">© 2019-20 Apuba. Design with <i class="mdi mdi-heart text-danger"></i> by <a>DigitalDots</a>.</p>
					</div>
				</div><!--end col-->
			</div><!--end row-->
		</div><!--end container-->
	</footer><!--end footer-->
	<!-- Footer End -->

	<!-- Back to top -->
	<a href="#" class="btn btn-icon btn-soft-primary back-to-top"><i data-feather="arrow-up" class="icons"></i></a>
	<!-- Back to top -->
</body>